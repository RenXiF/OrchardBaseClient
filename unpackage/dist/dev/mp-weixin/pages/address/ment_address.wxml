<view class="management_address_page"><block wx:for="{{addressList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{addressList.length!=0}}"><view data-event-opts="{{[['tap',[['setAddr',['$0'],[[['addressList','',index]]]]]]]}}" class="management_item border_bottom flex_rows" bindtap="__e"><view class="item_left"><view class="left_top flex_rows"><text hidden="{{!(item.addressState)}}" class="top_icon">默认</text><text class="top_text u-m-l-10">{{item.detailedAddress}}</text></view><view class="left_bottom flex_rows"><view class="bottom_name">{{item.consigneeName}}</view><view class="bottom_phone">{{item.phone}}</view></view></view><view data-event-opts="{{[['tap',[['edit',['$0'],[[['addressList','',index]]]]]]]}}" class="item_right" bindtap="__e">编辑</view></view></block></block><view data-event-opts="{{[['tap',[['doUrl',['pages/address/add_address']]]]]}}" class="management_button" bindtap="__e">新增地址</view></view>