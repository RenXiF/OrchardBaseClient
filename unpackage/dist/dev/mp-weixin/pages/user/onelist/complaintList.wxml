<view class="flex_columns"><view class="comhome"><block wx:for="{{datalist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{datalist.length!=0}}"><view class="comhome_bock flex_between flex_center u-font-md u-p-30 box_shadow "><view class="flex_columns"><text>{{"姓名："+item.usrNaem}}</text><text>{{"电话："+item.usrPhone}}</text><text>{{"内容："+item.question}}</text></view><view class="flex_row_reverse u-p-10"><block wx:if="{{item.status==0}}"><u-tag vue-id="{{'e1b6115a-1-'+index}}" text="待审核" mode="light" type="error" bind:__l="__l"></u-tag></block><block wx:else><u-tag vue-id="{{'e1b6115a-2-'+index}}" text="已审核" mode="light" type="success" bind:__l="__l"></u-tag></block></view></view></block></block><block wx:if="{{datalist.length==0}}"><u-empty vue-id="e1b6115a-3" text="暂无记录" mode="list" bind:__l="__l"></u-empty></block></view><u-gap vue-id="e1b6115a-4" height="80" bg-color="#f8f8f8" bind:__l="__l"></u-gap><view class="foot"><u-button vue-id="e1b6115a-5" type="warning" data-event-opts="{{[['^click',[['doUrl',['pages/user/onelist/complaint']]]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}">添加反馈</u-button></view></view>